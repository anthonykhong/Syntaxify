<%- include('../../partials/header') %>

<h1>SYNTAXIFY</h1>

<table id="list">
  <thead>
    <tr>
      <h2>Languages</h2>
      <td><a href="/documentations/languages/new">Add a Language</a></td>
    </tr>
  </thead>
  <tbody>
    <% languages.forEach(function(l) { %>
    <tr>
      <td><%= l.title %></td>
      <td><a href="/documentations/languages/<%= l._id %>">MORE DETAILS</a></td>
    </tr>
    <% }); %>
  </tbody>
</table>

<%- include('../../partials/footer') %>
