<%- include('../../partials/header') %>

<h1>SYNTAXIFY</h1>
<table id="list">
  <thead>
    <tr>
      <h2>Databases</h2>
      <td><a href="/documentations/databases/new">Add a Database</a></td>
    </tr>
  </thead>
  <tbody>
    <% databases.forEach(function(d) { %>
    <tr>
      <td><%= d.title %></td>
      <td><a href="/documentations/databases/<%= d._id %>">MORE DETAILS</a></td>
    </tr>
    <% }); %>
  </tbody>
</table>

<%- include('../../partials/footer') %>
